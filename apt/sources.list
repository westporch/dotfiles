# /etc/apt/sources.list for Debian 11(Bullseye)

deb http://mirror.debianusers.or.kr/debian/ bullseye main
deb-src http://mirror.debianusers.or.kr/debian/ bullseye main

deb http://mirror.debianusers.or.kr/debian-security bullseye-security main contrib
deb-src http://mirror.debianusers.or.kr/debian-security bullseye-security main contrib

# bullseye-updates, to get updates before a point release is made;
# see https://www.debian.org/doc/manuals/debian-reference/ch02.en.html#_updates_and_backports
deb http://mirror.debianusers.or.kr/debian/ bullseye-updates main contrib
deb-src http://mirror.debianusers.or.kr/debian bullseye-updates main contrib

# Opera browser
deb http://deb.opera.com/opera-stable/ stable non-free
